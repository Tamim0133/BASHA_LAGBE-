{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#008080",
          "image": "./assets/images/s2.png",
          "resizeMode": "cover"
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "backgroundColor": "#008080",
      "image": "./assets/images/s2.png",
      "resizeMode": "cover"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "statusBar": {
      "style": "dark",
      "backgroundColor": "transparent",
      "translucent": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.example.bashalagbe",
      "googleServicesFile": "./google-services.json"
    },
    "extra": {
      "eas": {
        "projectId": "6984c218-4bff-4a1d-b982-70bde41c3710"
      }
    },
    "owner": "dusty_spider"
  }
}
